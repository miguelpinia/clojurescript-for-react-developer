{:builds
 {:app {:asset-path "/js"
        :modules {:main {:init-fn app.core/main}}
        :output-dir "public/js"
        :target :browser}
  :cards {:asset-path "/js"
          :modules {:main {:init-fn app.cards/main}}
          :compiler-options {:devcards true}
          :output-dir "public/js"
          :target :browser}
  :test {:ns-regexp "app.cards"
         :output-to "out/test.js"
         :target :node-test}
  :e2e {:ns-regexp "e2e.*"
        :output-to "out/e2e.js"
        :target :node-test}}
 :dependencies [[reagent "0.8.1"]
                [cljs-ajax "0.8.4"]
                ;; main routing lib
                [metosin/reitit "0.5.18"]
                ;; spec: for data coercion/validation
                [metosin/reitit-spec "0.5.18"]
                ;; for frontend routing
                [metosin/reitit-frontend "0.5.18"]
                [devcards "0.2.6"]]
 :dev-http {4200 "public"}
 :nrepl {:port 3333}
 :source-paths ["src"]}
